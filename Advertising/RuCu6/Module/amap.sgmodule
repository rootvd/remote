[Rule]
AND,((URL-REGEX,"^http:\/\/(?:[\d\.]+|amdc\.m\.taobao\.com)\/amdc\/mobileDispatch"),(USER-AGENT,"AMapiPhone*")),REJECT

[Map Local]
